<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape android:shape="rectangle">
            <!-- 填充颜色 -->
            <solid android:color="@color/white_fa" />
            <!-- 矩形的圆角半径 -->
            <corners android:radius="4dp" />
            <!-- 描邊 -->
            <stroke android:width="1dp" android:color="#19000000" android:dashWidth="8dp" android:dashGap="0dp" />
        </shape>
    </item>

    <item android:state_pressed="true" >
        <shape android:shape="rectangle">
            <!-- 填充颜色 -->
            <solid android:color="@color/gray_0d" />
            <!-- 矩形的圆角半径 -->
            <corners android:radius="4dp" />
            <!-- 描邊 -->
            <stroke android:width="1dp" android:color="@color/gray_0d" android:dashWidth="8dp" android:dashGap="0dp" />

        </shape>
    </item>
</selector>

